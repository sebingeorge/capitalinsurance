@model List<Capital.Domain.PolicyIssue>
@{
    ViewBag.Title = "PendingPolicyList";
}

<script type="text/javascript">
    function Exit() {
        var url = '@Url.Action("Index","Home")';
        window.location = url;
    }
</script>


<div class="container content-in">
    <div class="row">
        <h1>Pending Endorsement List</h1>
    </div>
    <div class="row">
        <div class="col-md-2">
            <div class="row">
                <div class="col-md-6">
                    <label>From</label>
                    <input type="text" class="form_input" />
                </div>
                <div class="col-md-6">
                    <label>To</label>
                    <input type="text" class="form_input" />
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="row">
                <div class="col-md-6">
                    <label>Client Name</label>
                    <input type="text" class="form_input" />
                </div>
                <div class="col-md-6">
                    <label>Excecutive</label>
                    <input type="text" class="form_input" />
                </div>
            </div>
        </div>
    </div>
    <br />
    @Html.Partial("_PolicyListGrid", Model)
    <div class="row">
        <hr />
        <div class="col-md-12 hrpart">
        <input type="button" class=" form-button right" onclick="Exit()" value="Exit" />
        </div>
    </div>
</div>

