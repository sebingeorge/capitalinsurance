
@{
    ViewBag.Title = "Designation";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="container content-in">
    <div class="row">
        <h1>Designation</h1>
    </div>
    <div class="row">
        <div class="col-md-4">
            <div class="row form-group">
                <div class="col-md-12">
                    <label>Designation Name</label>
                    <input type="text" class="form_input" />
                </div>

                <div class="col-md-12">
                    <label>Remarks</label>
                    <textarea class="form_input text-area"></textarea>
                   
                </div>


            </div>
        </div>

        

    </div>
    <div class="row">
        <hr />
        <div class="col-md-offset-8 col-md-4">
            <button type="button" class="form-button right">Save</button>
            <button type="button" class="form-button right" onclick="Exit()">Exit</button>
        </div>

    </div>
</div>


